<%
const site = {
  name: 'Inventory',
  description: 'Inventory Management.',
  github: "https://xtreemze.github.io/inventory/",
}
%>
  <!DOCTYPE html>
  <html lang="en">

  <head>
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
    <title>${ site.name }</title>
    <meta name="viewport" content="width=device-width, user-scalable=yes, initial-scale=1.0, minimum-scale=0.0, maximum-scale=10.0">
    <link rel="canonical" href="${ site.github }" />
    <link rel="manifest" href="${ site.github }manifest.json">
  </head>

  <body class="grey lighten-3">

    <main class="row">
      <section class="col s12 m6 l4" id="spaces">
        <span class="row">
          <h3 class="grey-text col s12">Spaces
            <i class="material-icons small">forum</i>
            <i class="material-icons small">hotel</i>
          </h3>
        </span>
      </section>
      <section class="col s12 m6 l4" id="transport">
        <span class="row">
          <h3 class="grey-text col s12">Transport
            <i class="material-icons small">directions_car</i>
            <i class="material-icons small">directions_bike</i>
          </h3>
        </span>
      </section>
      <section class="col s12 m6 l4" id="production">
        <span class="row">
          <h3 class="grey-text col s12">Production
            <i class="material-icons small">photo_camera</i>
            <i class="material-icons small">panorama</i>
          </h3>
        </span>
      </section>
    </main>

  </body>

  </html>
